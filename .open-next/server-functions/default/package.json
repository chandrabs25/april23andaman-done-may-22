{
  "name": "andaman-travel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:opennext": "opennextjs-cloudflare build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:migrate": "wrangler d1 migrations apply travel_database --remote",
    "db:reset": "wrangler d1 delete travel_database",
    "db:seed": "wrangler d1 execute travel_database --file seed.sql --remote",
    "db:migratelocal": "wrangler d1 migrations apply travel_database --local",
    "db:seedlocal": "wrangler d1 execute travel_database --file seed.sql --local"
  },
